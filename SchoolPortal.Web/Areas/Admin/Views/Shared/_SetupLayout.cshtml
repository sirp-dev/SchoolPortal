<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Portal | Setup</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="icon" href="http://v1.exwhyzee.ng/admin/schoolportal.png" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Site</title>
    @*//@Scripts.Render("~/bundles/modernizr")*@
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/dist/css/skins/_all-skins.min.css">
    @RenderSection("styles", false)

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <span class="logo-mini"><b>Non</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>NON</b></span>

            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @using (Html.BeginForm("LogOff", "XYZ", new { area = "Account" }, FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @*@Html.AntiForgeryToken()*@
                            <li class="dropdown user user-menu">
                                <a href="javascript:document.getElementById('logoutForm').submit()" style="position: relative;color:#fff; text-decoration:none; display: block;padding: 10px 15px;">

                                    Sign Out
                                </a>

                            </li>
                        }
                      
                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Advance Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
               
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->

            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Dashboard
                    <small>Control panel</small>
                </h1>

            </section>

            <!-- Main content -->
            <section class="content">
                @RenderBody()

            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
        </footer>


    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="http://v1.exwhyzee.ng/admin/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="http://v1.exwhyzee.ng/admin/dist/js/adminlte.min.js"></script>
    <!-- Sparkline -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- jvectormap  -->
    <script src="http://v1.exwhyzee.ng/admin/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="http://v1.exwhyzee.ng/admin/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- SlimScroll -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- ChartJS -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/chart.js/Chart.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="http://v1.exwhyzee.ng/admin/dist/js/pages/dashboard2.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="http://v1.exwhyzee.ng/admin/dist/js/demo.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
