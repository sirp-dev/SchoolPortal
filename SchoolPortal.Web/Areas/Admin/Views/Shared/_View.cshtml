<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Portal | @ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->

    <link rel="icon" href="http://v1.exwhyzee.ng/admin/schoolportal.png" type="image/x-icon" />
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/dist/css/AdminLTE.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/plugins/iCheck/square/blue.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition register-page">

    <div class="register-box" style="width:75% !important;margin:2% auto !important;">
        <div class="register-logo">
           
            <a href="/"><b>
    @Html.Partial("~/Areas/Admin/Views/Shared/_SchoolName.cshtml")
</b></a>
        </div>
        <!-- /.login-logo -->
        <div class="register-box-body">

            @RenderBody()
            <!-- /.social-auth-links -->



        </div>
        <h6 class="text-center"><a href="http://v1.exwhyzee.ng/" target="_blank">Powered By: <span class="text-danger">Exwhyzee Technologies</span></a></h6>
        <!-- /.login-box-body -->

    </div>
    <!-- /.login-box -->
    <!-- jQuery 3 -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="http://v1.exwhyzee.ng/admin//bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- iCheck -->
    <script src="http://v1.exwhyzee.ng/admin/plugins/iCheck/icheck.min.js"></script>
    <script>
  $(function () {
    $('input').iCheck({
      checkboxClass: 'icheckbox_square-blue',
      radioClass: 'iradio_square-blue',
      increaseArea: '20%' /* optional */
    });
  });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
