
@{
    ViewBag.Title = "Document";
}





<div class="row">
    <!-- left column -->
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <!-- general form elements -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">New</h3>
                @if (TempData["error"] != null || TempData["error1"] != null || TempData["batcherror"] != null)
                {
                    <div class="alert alert-danger alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                        @Html.Raw(TempData["batcherror"])<br />
                        @TempData["error"] <br />
                        @TempData["error1"]
                    </div>
                }
                @if (TempData["msg"] != null)
                {
                    <div class="alert alert-success alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                        @TempData["msg"]
                    </div>
                }
            </div>
            @using (Html.BeginForm("Document", "UserManagers", FormMethod.Post, new { @class = "", role = "form", enctype = "multipart/form-data" }))
            {
                @*@Html.AntiForgeryToken()*@


                <div class="box-body">

                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                   

                </div>

                <div class="box-footer">
                    <input type="file" class="form-control" name="excelfile" style="margin-bottom:20px;" />
                    <input type="submit" value="Upload" class="btn btn-sm btn-primary" />
                    <a href="javascript: history.back()" class="btn btn-default">Cancel</a>
                </div>

            }
        </div>

    </div>
    <div class="col-md-2"></div>

</div>