                                                        <th style="padding:2px 6px;">@asdf.LowerLimit - @asdf.UpperLimit</th>
                                                        <th style="padding:2px 6px;"> @asdf.Remark</th>
                                                    </tr>
                                                }

                                            </table>
                                        </th>
                                        <th>
                                            <table style="width:100%;text-align:left;padding-left:10px;text-transform:uppercase !important;" align="center" class="table-bordered">
                                                <tr style="">
                                                    <th style="padding-left:10px;padding-top:10px;padding-bottom:10px;text-transform:uppercase;">FULL NAME</th>
                                                    <th style="padding-left:10px;padding-top:10px;padding-bottom:10px;">@res.Fullname</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding-left:10px;">REG. NUMBER</th>
                                                    <th style="padding-left:10px;">@res.RegNumber</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding-left:10px;">CLASS</th>
                                                    <th style="padding-left:10px;">@res.ClassName</th>
                                                </tr>
                                                <tr class="done">
                                                    <th>

                                                        <table style="width:100%;">
                                                            <tr>
                                                                <td style="padding-left:10px;width:50%;border-right:1px solid #dee2e6;">AVERAGE</td>
                                                                <td style="padding-left:10px;width:50%;">@res.Average</td>
                                                            </tr>
                                                        </table>
                                                    </th>
                                                    <th>
                                                        <table style="width:100%;">
                                                            <tr>
                                                                <td style="padding-left:10px;width:50%;border-right:1px solid #dee2e6;">TOTAL</td>
                                                                <td style="padding-left:10px;width:50%;">@Math.Round(res.TotalScore ?? 0)/@Math.Round(res.OverallScore ?? 0)</td>

                                                            </tr>
                                                        </table>
                                                    </th>
                                                </tr>
                                                @if (res.showPosOnClassResult == true)
                                                {
                                                    <tr class="done">
                                                        <th>
                                                            <table style="width:100%;">
                                                                <tr>
                                                                    <td style="padding-left:10px;width:50%;border-right:1px solid #dee2e6;">POSITION</td>
                                                                    <td style="padding-left:10px;width:50%;">
                                                                        @{mp++;}


                                                                        @if (res.ShowAverageOverPositionInClass == true)
                                                                        {
                                                                            <span style="text-transform:uppercase">@res.AveragePositionGrade</span> <text> (@res.AveragePositionRemark) </text>

                                                                        }
                                                                        else
                                                                        {

                                                                            if (res.Position == 1)
                                                                            {
                                                                                @res.Position
                                                                                <spamp><sup>st</sup></spamp>
                                                                            }
                                                                            else if (res.Position == 2)
                                                                            {
                                                                                @res.Position
                                                                                <spamp><sup>nd</sup></spamp>
                                                                            }
                                                                            else if (res.Position == 3)
                                                                            {
                                                                                @res.Position
                                                                                <spamp><sup>rd</sup></spamp>
                                                                            }
                                                                            else
                                                                            {
                                                                                @res.Position
                                                                                <spamp><sup>th</sup></spamp>
                                                                            }


                                                                            <span style="text-transform:uppercase">OUT OF</span> @res.TotalStudent @GeneralService.StudentorPupil()<text>S IN CLASS</text>
                                                                        }
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </th>
                                                        <th>
                                                            <table style="width:100%;">
                                                                <tr>
                                                                    <td style="padding-left:10px;width:50%;;border-right:1px solid #dee2e6;">REMARK</td>
                                                                    <td style="padding-left:10px;width:50%;">@res.ResultRemark</td>
                                                                </tr>
                                                            </table>
                                                        </th>
                                                    </tr>
                                                }

                                                else
                                                {

                                                    <tr>
                                                        <td style="padding-left:10px;width:50%;border-right:1px solid #dee2e6;">REMARK</td>
                                                        <td style="padding-left:10px;width:50%;">@res.ResultRemark</td>
                                                    </tr>
                                                }

                                            </table>

                                        </th>
                                        <th>
                                            <table class="table-bordered" align="center" style="font-size:12px !important;">
                                                <tr style="border:1px solid;text-align:center;">
                                                    <th colspan="2">Cognitive Domain</th>

                                                </tr>

                                                <tr>
                                                    <th style="padding:2px 6px;">Analyzing</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Analyzing</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding:2px 6px;">Application</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Application</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding:2px 6px;">Creativity</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Creativity</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding:2px 6px;">Evaluation</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Evaluation</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding:2px 6px;">Rememberance</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Rememberance</th>
                                                </tr>
                                                <tr>
                                                    <th style="padding:2px 6px;">Understanding</th>
                                                    <th style="padding:2px 6px;">@res.RecognitiveDomain.Understanding</th>
                                                </tr>


                                            </table>
                                        </th>
                                    </tr>
                                </table>
                                <br />
                                <br />
                                <table style="width:100%;" class="table-bordered">
                                    <tr>
                                        <th>
                                            SUBJECT
                                        </th>
                                        @*<th>C.A</th>
                                <th>EXAM</th>*@
                                        @if (GeneralService.EnabledProjectScore() == true)
                                        {
                                            <th>PROJECT</th>
                                        }
                                        @if (GeneralService.EnabledTestScore() == true && GeneralService.EnabledTestScore2() == true)
                                        {
                                            <th>1ST TEST</th>
                                        }
                                        else
                                        {
                                            <th>C.A</th>
                                        }

                                        @if (GeneralService.EnabledTestScore2() == true)
                                        {
                                            <th>2ND TEST</th>
                                        }

                                        @if (GeneralService.EnabledClassExerciseScore() == true)
                                        {

                                            <th>CLASS EXERCISE</th>
                                        }

                                        @if (GeneralService.EnabledAssessmentScore() == true)
                                        {

                                            <th>ASSESSMENT</th>
                                        }
                                        @if (GeneralService.TotalCAScore(ViewBag.session.Id) != null || GeneralService.TotalCAScore(ViewBag.session.Id) != 0)
                                        {
                                            <th>TOTAL C.A</th>


                                        }
                                        else
                                        {

                                        }

                                        @if (GeneralService.EnabledExamScore() == true)
                                        {

                                            <th>EXAM</th>
                                        }
                                        <th>TOTAL SCORE</th>
                                        <th>GRADE</th>
                                        <th>REMARK</th>
                                    </tr>
                                    @foreach (var item in res.SubjectList)
                                    {
                                        <tr>
                                            <td style="padding:3px;">@item.Subject.SubjectName</td>
                                            @*<td style="padding:3px;">@Math.Round(item.TestScore ?? 0)</td>
                                    <td style="padding:3px;">@Math.Round(item.ExamScore ?? 0)</td>*@
                                            @if (GeneralService.EnabledProjectScore() == true)
                                            {
                                                <td style="padding:3px;">@item.Project</td>
                                            }
                                            @if (GeneralService.EnabledTestScore() == true)
                                            {
                                                <td style="padding:3px;">@item.TestScore</td>

                                            }

                                            @if (GeneralService.EnabledTestScore2() == true)
                                            {
                                                <td style="padding:3px;">@item.TestScore2</td>
                                            }

                                            @if (GeneralService.EnabledClassExerciseScore() == true)
                                            {

                                                <td style="padding:3px;">@item.ClassExercise</td>
                                            }

                                            @if (GeneralService.EnabledAssessmentScore() == true)
                                            {

                                                <td style="padding:3px;">@item.Assessment</td>
                                            }
                                            @if (GeneralService.TotalCAScore(ViewBag.session.Id) != null || GeneralService.TotalCAScore(ViewBag.session.Id) != 0)
                                            {
                                                <td style="padding:3px;">@item.TotalCA</td>
                                            }
                                            else
                                            {

                                            }

                                            @if (GeneralService.EnabledExamScore() == true)
                                            {

                                                <td style="padding:3px;">@item.ExamScore</td>
                                            }
                                            <td style="padding:3px;">@Math.Round(item.TotalScore ?? 0)</td>
                                            <td style="padding:3px;">@item.Grade</td>
                                            <td style="padding:3px;">@item.Remark</td>
                                        </tr>
                                    }
                                </table>
                                <div style="float:right;margin-top:10px;text-align:right;padding-right:5px;">
                                    @if (res.SchoolLogo != null)
                                    {
                                        <img class="img-circle" src="data:image/jpg;base64,@(Convert.ToBase64String(res.SchoolStamp))" alt="" style="height:70px;">
                                    }
                                    <h5 style="margin-bottom:1px;padding-bottom:2px;">@res.headteacher</h5>
                                    <h5 style="margin-top:2px;padding-top:2px;line-height:1.4px;">@GeneralService.HeadteacherOrPrincipal()</h5>
                                </div>

                            </div>
                        </div>
                        <br clear="all" style="page-break-before:always" />

                        <div class="clearfix"></div>
                        <hr style="border:5px solid #007acc;" />
                    }
                }
            </div>
        </div>
    </div>
</div>


