@model SchoolPortal.Web.Models.Dtos.SettingLayoutDto
@using SchoolPortal.Web.Areas.Service

@if (Model.Image == null)
{
    <link rel="icon" href="http://v1.exwhyzee.ng/admin/schoolportal.png" type="image/jpg">

}
else
{
    <link rel="icon" href="data:image/jpg;base64,@(Convert.ToBase64String(Model.Image))" type="image/x-icon" />
}
