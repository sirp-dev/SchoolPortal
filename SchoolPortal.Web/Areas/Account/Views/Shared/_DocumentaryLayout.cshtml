<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>

        @if (User.IsInRole("Staff"))
        {
            <text>Staff</text>
        }
        @if (User.IsInRole("Student"))
        {
            <text>Student</text>
        }


        @if (User.IsInRole("SuperAdmin") || User.IsInRole("Admin"))
        {
            <text>Admin</text>
        }

        Portal | @ViewBag.Title | @Html.Partial("http://v1.exwhyzee.ng/Areas/Admin/Views/Shared/_SchoolName.cshtml")
    </title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="icon" href="http://v1.exwhyzee.ng/admin/schoolportal.png" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @Scripts.Render("http://v1.exwhyzee.ng/bundles/modernizr")
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/bootstrap/dist/css/bootstrap.min.css">

    @*<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet">*@
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    @*<link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/font-awesome/css/font-awesome.min.css">*@
    <!-- Ionicons -->
    @*<link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/Ionicons/css/ionicons.min.css">*@
    <link href="https://unpkg.com/ionicons@4.4.6/dist/css/ionicons.min.css" rel="stylesheet">

    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/plugins/iCheck/all.css">
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Theme style -->
    @RenderSection("Style", required: false)
    @RenderSection("Styles", required: false)

    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="http://v1.exwhyzee.ng/admin/dist/css/skins/_all-skins.min.css">

    <link href="http://v1.exwhyzee.ng/admin/main.css" rel="stylesheet" />

    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <style>
        .btn-mobile {
            padding: 2px 6px !important;
            font-size: 12px !important;
            font-weight: 200 !important;
            white-space: normal;
        }
    </style>
    <style>
        .example-modal .modal {
            position: relative;
            top: auto;
            bottom: auto;
            right: auto;
            left: auto;
            display: block;
            z-index: 1;
        }

        .example-modal .modal {
            background: transparent !important;
        }

        .direct-chat-messages {
            height: auto !important;
        }

        .row {
            margin-right: 0px !important;
            margin-left: 0px !important;
        }

        tr > th, tr > th, tr > th, tr > td, tr > td, tr > td {
            border: 1px solid #c1c1c1 !important;
        }

        table td {
            white-space: nowrap
        }


        @@media print {
            /*@@page { margin: 0; }
            body { margin: 1.6cm; }*/
            table {
                border: solid #000 !important;
                border-width: 1px 0 0 1px !important;
                page-break-inside: avoid;
                margin: 0;
            }

            th, td {
                border: solid #000 !important;
                border-width: 0 1px 1px 0 !important;
                margin: 0;
            }
        }



        /* Fixed/sticky icon bar (vertically aligned 50% from the top of the screen) */
        .icon-bar {
            position: fixed;
            top: 30%;
            -webkit-transform: translateY(50%);
            -ms-transform: translateY(50%);
            transform: translateY(50%);
        }

            /* Style the icon bar links */
            .icon-bar a {
                display: block;
                text-align: center;
                padding: 16px;
                transition: all 0.3s ease;
                color: white;
                font-size: 20px;
            }

                /* Style the social media icons with color, if you want */
                .icon-bar a:hover {
                    background-color: #000;
                }

        .buttons {
            background: #3B5998;
            color: white;
        }

        .wrapper {
            overflow: visible;
        }

        .skin-blue .sidebar-form input[type="text"] {
            color: white;
            font-weight: 800;
        }

        /*'''''*/

        /*'''''''*/

        .sticky-container {
            padding: 0px;
            margin: 0px;
            position: fixed;
            right: -110px;
            top: 280px;
            width: 220px;
            z-index: 1100;
        }

        .sticky li {
            list-style-type: none;
            background-color: #fff;
            color: #efefef;
            height: 43px;
            padding: 0px;
            padding-left: 5px;
            margin: 0px 0px 1px 0px;
            -webkit-transition: all 0.25s ease-in-out;
            -moz-transition: all 0.25s ease-in-out;
            -o-transition: all 0.25s ease-in-out;
            transition: all 0.25s ease-in-out;
            cursor: pointer;
        }

            .sticky li:hover {
                margin-left: -115px;
            }

            .sticky li img {
                float: left;
                margin: 5px 4px;
                margin-right: 5px;
            }

            .sticky li p {
                padding-top: 5px;
                margin: 0px;
                line-height: 16px;
                font-size: 11px;
            }

                .sticky li p a {
                    text-decoration: none;
                    color: #2C3539;
                }

                    .sticky li p a:hover {
                        text-decoration: underline;
                    }
    </style>

    <style>
        @@media print {
            .noprint {
                display: none !important;
            }

            html {
                zoom: 75%;
                font-size: 55% !important;
            }
        }
    </style>

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="" class="logo">
                @Html.Action("LayoutProfileTitle", "PartialView", new { Area = "Admin" })


            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->


                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                @*<a>
                        @Html.Action("LayoutSchoolName", "PartialView", new { Area = "Admin" })
                    </a>*@
                @using SchoolPortal.Web.Areas.Service
                <div class="navbar-custom-menu" style="float:none !important;margin-left:30px !important;">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        @*<li>
                                <a>Quote of the day ( @GeneralService.Quote() )</a>
                            </li>*@
                    </ul>
                </div>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @using (Html.BeginForm("LogOff", "XYZ", new { area = "Account" }, FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @*@Html.AntiForgeryToken()*@
                            <li class="dropdown user user-menu">
                                <a href="javascript:document.getElementById('logoutForm').submit()" style="position: relative;color:#fff; text-decoration:none; display: block;padding: 10px 15px;">

                                    Sign Out
                                </a>

                            </li>
                        }

                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                @Html.Action("LayoutProfile", "PartialView", new { Area = "Admin" })
                <!-- search form  <a href="#"><i class="fa fa-circle text-success"></i> Online</a> -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="" value="Online" class="form-control" disabled="disabled">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-circle text-success"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->

                <ul class="sidebar-menu" data-widget="tree">

                    @*@Html.Partial("_DocumentaryMenu")*@
                    @Html.Action("_DocumentaryMenu", "Doc", new { area = "Documentaries" })
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Dashboard
                    <small><strong> @Html.Partial("http://v1.exwhyzee.ng/Areas/Admin/Views/Shared/_currentSession.cshtml") Term</strong></small>
                </h1>
                @{
                    HttpRequest req = System.Web.HttpContext.Current.Request;
                    string browserName = req.Browser.Browser;
                }

                <ol class="breadcrumb">
                    <li>
                        <a>
                            <span><strong>Current Browser: @browserName</strong></span>
                        </a>
                        <a href="http://support.esp.exwhyzee.ng/" target="_blank">
                            <span class="btn btn-primary">Help Documents </span>
                        </a>
                        <a href="https://www.youtube.com/channel/UCfRRuNhVsN_9huTAGSR5V-g/" target="_blank">
                            <span class="btn btn-primary">Help Videos </span>
                        </a>
                        <a href="javascript: history.back()">
                            <span class="btn btn-primary"> <i class="fa fa-arrow-left"></i> Back</span>
                        </a>

                    </li>

                </ol>
            </section>
            <section>
                <h4 style="padding-left:20px;font-weight:900;">
                    @Html.Partial("http://v1.exwhyzee.ng/Areas/Admin/Views/Shared/_SchoolName.cshtml")
                </h4>
            </section>

            <!-- Main content -->

            <section class="content">
                @RenderBody()

            </section>
            <!-- /.content -->
        </div>
       

        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Powered by:</b> <a href="http://v1.exwhyzee.ng/">Exwhyzee Technologies</a>
            </div>
            <strong class="noprint">
                Copyright &copy; @DateTime.UtcNow.Year <a href="http://iskools.com/">iSkools Portal</a>.  All rights
                reserved.
            </strong>
        </footer>


    </div>

    <!-- jQuery 3 -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="http://v1.exwhyzee.ng/Content/ExcelExport/jquery.table2excel.min.js"></script>
    <script src="http://v1.exwhyzee.ng/admin/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="http://v1.exwhyzee.ng/admin/plugins/jQueryUI/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->

    @RenderSection("Scripts", required: false)
    <!-- Slimscroll -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="http://v1.exwhyzee.ng/admin/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- iCheck 1.0.1 -->
    <script src="http://v1.exwhyzee.ng/admin/plugins/iCheck/icheck.min.js"></script>
    <!-- FastClick -->
    <script src="http://v1.exwhyzee.ng/admin/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="http://v1.exwhyzee.ng/admin/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="http://v1.exwhyzee.ng/admin/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="http://v1.exwhyzee.ng/admin/dist/js/demo.js"></script>
    <script>
        //iCheck for checkbox and radio inputs
        $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
            checkboxClass: 'icheckbox_minimal-blue',
            radioClass: 'iradio_minimal-blue'
        })
        //Red color scheme for iCheck
        $('input[type="checkbox"].minimal-red, input[type="radio"].minimal-red').iCheck({
            checkboxClass: 'icheckbox_minimal-red',
            radioClass: 'iradio_minimal-red'
        })
        //Flat red color scheme for iCheck
        $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
            checkboxClass: 'icheckbox_flat-green',
            radioClass: 'iradio_flat-green'
        })
    </script>
    <script>
        //Date picker
        $('#datepicker').datepicker({
            autoclose: true,
            dateFormat: 'MM-dd-yyyy'
        })

        //Date picker
        $('#datepicker1').datepicker({
            autoclose: true,
            dateFormat: 'MM-dd-yyyy'
        })
        //var date = $('#datepicker').datepicker({ dateFormat: 'dd-mm-yy' }).val();
    </script>
    @RenderSection("Scripts2", required: false)

</body>
</html>
